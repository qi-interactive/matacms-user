{
    "name": "matacms/matacms-user",
    "description": "Flexible user registration and authentication module for MATA CMS",
    "keywords": ["yii2", "matacms-user", "user management", "mata cms"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
    },
    "minimum-stability": "dev",
    "require": {
        "mata/mata-user": "~dev-development",
        "matacms/matacms-rbac": "dev-development",
        "matacms/matacms-base": "dev-development"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "matacms\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "matacms\\user\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
